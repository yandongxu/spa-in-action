{
    "extends": "airbnb-base",
    "plugins": [
        "html"
    ],
    "rules": {
        "no-param-reassign": [0],
        "comma-dangle": [0],
        "indent": [2, 4, { "SwitchCase": 1 }],
        "no-multi-spaces": 2,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-console": [0],
        "space-before-function-paren": [2, {
            "anonymous": "never",
            "named": "always"
        }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jasmine": true
    }
}
